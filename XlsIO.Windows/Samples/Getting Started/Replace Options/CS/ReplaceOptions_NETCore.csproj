<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>ReplaceOptions</AssemblyName>
    <RootNamespace>ReportFromTemplate</RootNamespace>
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
    <Compile Remove="Properties\Resources.Designer.cs" />
    <Compile Remove="Properties\Settings.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="Properties\Resources.resx" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Properties\Settings.settings" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Compression.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Grid.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SpellChecker.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.XlsIO.WinForms" Version="17.3.0.14" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
</Project>