<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>ExceltoPDF</AssemblyName>
    <RootNamespace>ExceltoPDF</RootNamespace>
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Form.cs" />
    <Compile Remove="Form.Designer.cs" />
    <Compile Remove="Program.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="Form.resx" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Fonts\bahnschrift.ttf" />
    <None Remove="Fonts\georgiab.ttf" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Fonts\bahnschrift.ttf" />
    <EmbeddedResource Include="Fonts\georgiab.ttf" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Compression.Base" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.ExcelChartToImageConverter.WinForms" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.ExcelToPdfConverter.WinForms" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Grid.Base" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Grid.Windows" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Licensing" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Pdf.WinForms" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.SpellChecker.Base" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Tools.Base" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Tools.Windows" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.XlsIO.WinForms" Version="20.2.0.36" />
  </ItemGroup>
</Project>